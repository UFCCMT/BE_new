#**************************************************************
# Set Maximum Delay (Setup)
#**************************************************************
set_max_delay -from [get_ports {j3_ext_io[*]}]    -to * 4.0
set_max_delay -from [get_ports {l[*]}]    -to * 4.0
set_max_delay -from [get_ports {l_in[*]}]    -to * 4.0
set_max_delay -from [get_ports {l_io[*]}]    -to * 4.0
#**************************************************************
# Set Minimum Delay (input Hold)
#**************************************************************
set_min_delay -from [get_ports {j3_ext_io[*]}]    -to * -1.75
set_min_delay -from [get_ports {l[*]}]    -to * -1.75
set_min_delay -from [get_ports {l_in[*]}]    -to * -1.75
set_min_delay -from [get_ports {l_io[*]}]    -to * -1.75
#**************************************************************
# Set Output Delay (Tco)
#**************************************************************
set_max_delay -from * -to [get_ports {j3_ext_io[*]}] 10.5
set_max_delay -from * -to [get_ports {j3_ext_io_dir[*]}] 10.5
set_max_delay -from * -to [get_ports {l[*]}] 10.5
set_max_delay -from * -to [get_ports {l_io[*]}] 10.5
set_max_delay -from * -to [get_ports {clk_out[*]}] 10.5
#**************************************************************
# Set Minimum Delay (output Hold)
#**************************************************************
set_min_delay -from * -to [get_ports {j3_ext_io[*]}]  -1.75
set_min_delay -from * -to [get_ports {j3_ext_io_dir[*]}]  -1.75
set_min_delay -from * -to [get_ports {l[*]}]  -1.75
set_min_delay -from * -to [get_ports {l_io[*]}]  -1.75
set_min_delay -from * -to [get_ports {clk_out[*]}]  -1.75
